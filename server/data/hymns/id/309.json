{
    "lang": "id",
    "num": "309",
    "oldnum": "309",
    "title": "Pengharapan di Tahun Baru",
    "verses": [
        {
            "label": "1",
            "num": 1,
            "html": "1<br><b>T</b>ahun baru <br><b>t</b>iba lagi,<br><b>k</b>ita kumpul <br><b>d</b>i rumahNya;<br><b>B</b>eribadah dan <br><b>b</b>ersyukur,<br><b>s</b>uara pujian <br><b>s</b>ampai surga."
        },
        {
            "label": "2",
            "num": 2,
            "html": "2<br><b>S</b>atu tahun <br><b>T</b>uhan jaga,<br><b>b</b>eri berkat <br><b>d</b>an pimpinan;<br><b>P</b>erjalanan ta-<br><b>h</b>un ini,<br><b>s</b>rahkan pada <br><b>p</b>impinanNya."
        },
        {
            "label": "3",
            "num": 3,
            "html": "3<br><b>T</b>uhan, jagalah <br><b>h</b>idupku,<br><b>a</b>garku me-<br><b>m</b>uliakanMu;<br><b>B</b>erikanku hik-<br><b>m</b>at, kuasa,<br><b>m</b>elakukan <br><b>k</b>ehendakMu."
        },
        {
            "label": "4",
            "num": 4,
            "html": "4<br><b>T</b>ingkatkanlah <br><b>i</b>man daku,<br><b>T</b>eguhkanlah <br><b>r</b>ohaniku;<br><b>P</b>elayanan ta-<br><b>h</b>un ini,<br><b>k</b>u serahkan <br><b>k</b>e tanganMu."
        }
    ]
}